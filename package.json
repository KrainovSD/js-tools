{
  "name": "krainovsd",
  "private": false,
  "scripts": {
    "build:js-helpers": "pnpm --filter=@krainovsd/js-helpers build",
    "build:eslint-presets": "pnpm --filter=@krainovsd/eslint-presets build",
    "build:react-icons": "pnpm --filter=@krainovsd/react-icons build",
    "build:react-ui": "pnpm --filter=@krainovsd/react-ui build",
    "format:js-helpers": "pnpm --filter=@krainovsd/js-helpers format",
    "format:eslint-presets": "pnpm --filter=@krainovsd/eslint-presets format",
    "format:react-icons": "pnpm --filter=@krainovsd/react-icons format",
    "format:react-ui": "pnpm --filter=@krainovsd/react-ui format",
    "lint:js-helpers": "pnpm --filter=@krainovsd/js-helpers lint",
    "lint:eslint-presets": "pnpm --filter=@krainovsd/eslint-presets lint",
    "lint:react-icons": "pnpm --filter=@krainovsd/react-icons lint",
    "lint:react-ui": "pnpm --filter=@krainovsd/react-ui lint",
    "test:js-helpers": "pnpm --filter=@krainovsd/js-helpers test",
    "dev:react-icons": "pnpm --filter=@krainovsd/react-icons dev",
    "start:react-ui": "pnpm --filter=@krainovsd/react-ui start",
    "publish:js-helpers": "pnpm --filter=@krainovsd/js-helpers publish-package",
    "publish:eslint-presets": "pnpm --filter=@krainovsd/eslint-presets publish-package",
    "publish:react-icons": "pnpm --filter=@krainovsd/react-icons publish-package",
    "publish:react-ui": "pnpm --filter=@krainovsd/react-ui publish-package",
    "lint": "turbo lint",
    "test": "turbo test",
    "build": "turbo build",
    "format": "turbo format",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "9.1.0",
    "turbo": "2.2.3"
  },
  "packageManager": "pnpm@9.4.0"
}
