{
  "name": "krainovsd",
  "private": false,
  "scripts": {
    "pkg-exec": "npx ksd exec-package-command",
    "dep-check": "npx ksd check-package-duplicate",
    "reset-deps": "npx ksd clear-dependencies && pnpm i",
    "lint": "turbo lint --ui tui --parallel",
    "test": "turbo test --ui tui --parallel",
    "build": "turbo build --ui tui --parallel",
    "format": "turbo format --ui tui --parallel",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@krainovsd/cli": "0.2.0",
    "husky": "9.1.7",
    "turbo": "2.4.4"
  },
  "packageManager": "pnpm@9.4.0"
}
